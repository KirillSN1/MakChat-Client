    "version": "0.2.0",
    "configurations": [
        {
            "name": "client debug",
            "request": "launch",
            "type": "dart",
            "toolArgs": ["--dart-define", "DEBUG=true"]
        },
        {
            "name": "local client debug",
            "request": "launch",
            "type": "dart",
            "toolArgs": [
                "--dart-define", "DEBUG=true",
                "--dart-define", "LOCAL=true"
            ]
        },
        {
            "name": "client (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "client (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}